/k "cls &&  echo [SYSTEM] Запуск протокола поздравления v3.2... &&  timeout /t 3 >nul &&  echo [BOOT] Инициализация эмоционального ядра... &&  echo [====          ] 30% &&  timeout /t 2 >nul &&  cls &&  echo [BOOT] Инициализация эмоционального ядра... &&  echo [========      ] 60% &&  timeout /t 2 >nul &&  cls &&  echo [BOOT] Инициализация эмоционального ядра... &&  echo [============  ] 90% &&  timeout /t 2 >nul &&  cls &&  echo [OK] Эмоциональное ядро загружено! &&  echo [INFO] Подготовка персонального поздравления... &&  timeout /t 4 >nul &&  cls &&  echo. &&  echo +----------------------------------------------+ &&  echo ^|       С ДНЁМ РОЖДЕНИЯ, ГЕРОЙ ЭТОГО ДНЯ!     ^| &&  echo +----------------------------------------------+ &&  echo. &&  echo [OK] Система зафиксировала новый цикл времени! &&  timeout /t 3 >nul &&  echo [INFO] Ещё одна страница в твоей книге жизни готова к записи. &&  timeout /t 4 >nul &&  cls &&  echo [LOADING] Активация философского модуля... &&  echo [===       ] 25% &&  timeout /t 2 >nul &&  cls &&  echo [LOADING] Активация философского модуля... &&  echo [========  ] 75% &&  timeout /t 2 >nul &&  cls &&  echo [LOADING] Активация философского модуля... &&  echo [==========] 100% &&  timeout /t 2 >nul &&  cls &&  echo [OK] Философский модуль готов! &&  echo. &&  echo Жизнь — как код, который никто не рефакторил: &&  echo иногда баги, иногда гениальные строки. &&  echo День рождения — это релиз новой версии тебя, &&  echo с патчами и парой недокументированных фич! &&  timeout /t 5 >nul &&  echo. &&  echo [HINT] Вселенная, похоже, подбросила тебе ещё один баг... &&  echo        Но ты же мастер дебага, правда? &&  timeout /t 4 >nul &&  cls &&  echo [INFO] Запуск визуального ядра... &&  timeout /t 3 >nul &&  echo. &&  echo     ^*^*^*^*^*^*^*^*^*^*^*^*^*^*^*^*^*^*^*^*^*^ &&  echo     ^*         С ДНЁМ РОЖДЕНИЯ!              *^ &&  echo     ^*         СИЯЙ, КАК ЗВЁЗДЫ!             *^ &&  echo     ^*^*^*^*^*^*^*^*^*^*^*^*^*^*^*^*^*^*^*^*^*^ &&  echo. &&  timeout /t 4 >nul &&  cls &&  echo [INPUT] Активация интерактивного режима... &&  timeout /t 3 >nul &&  echo. &&  echo Какую новую фичу ты добавишь в свою книгу жизни? &&  echo (Может, патч для счастья или багфикс рутины?) &&  set /p ANSWER=Твой ответ: &&  echo. &&  echo [OK] Ответ зафиксирован: %ANSWER% &&  timeout /t 4 >nul &&  cls &&  echo [FINAL] Запуск финального протокола... &&  echo [=====     ] 50% &&  timeout /t 2 >nul &&  cls &&  echo [FINAL] Запуск финального протокола... &&  echo [==========] 100% &&  timeout /t 2 >nul &&  cls &&  echo +----------------------------------------------+ &&  echo ^|         ПУСТЬ ТВОЙ КОД ВСЕГДА КОМПИЛИТСЯ!    ^| &&  echo +----------------------------------------------+ &&  echo. &&  echo [OK] С днём рождения, архитектор своей судьбы! &&  echo Пусть каждая строка твоей жизни будет без багов, &&  echo а каждая новая версия — шедевром! &&  timeout /t 5 >nul &&  echo. &&  echo [HINT] Если жизнь зависла, просто перезагрузи мечты. &&  timeout /t 4 >nul &&  echo. &&  echo [INFO] Протокол поздравления завершён. &&  pause && exit" 
